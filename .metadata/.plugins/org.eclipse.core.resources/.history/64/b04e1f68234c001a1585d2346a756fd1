package journal.dev;

class Complex {
	private double re, im;

	public Complex(double re, double im) {
		this.re = re;
		this.im = im;
	}
}

public class OverridingToStringConstructorExample {

	public static void main(String[] args) {
		Complex c1 = new Complex(10, 15);
		System.out.println(c1.toString());

	}

}
